{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["plugin:react/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["react", "@typescript-eslint"],
    "rules": {
      "react/jsx-pascal-case": 0,      
      "import/no-anonymous-default-export": "off",
      "@typescript-eslint/no-empty-interface": "off"
    }
  }
  

